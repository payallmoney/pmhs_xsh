<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.net.tongfang.framework.security.vo.TaskCat" table="Task_Cat" >
        <id name="id">
            <column name="id" sql-type="nvarchar" not-null="true"/>
        </id>
        <property name="name">
            <column name="name" sql-type="nvarchar"/>
        </property>
        <property name="ord">
            <column name="ord" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>